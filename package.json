{
  "name": "carbon",
  "version": "0.1.0",
  "description": "React app boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "parcel ./src/index.html -d build/ -p 3000 --no-autoinstall",
    "prebuild": "npm run clean",
    "build": "parcel build src/index.html -d build/",
    "postbuild": "react-snap",
    "clean": "rimraf build && rimraf .cache",
    "lint": "eslint -c .eslintrc.yml src"
  },
  "keywords": [
    "React",
    "Boilerplate",
    "Styled Components"
  ],
  "alias": {
    "containers": "./src/containers",
    "components": "./src/components",
    "assets": "./src/assets",
    "styles": "./src/styles"
  },
  "author": "Leadhome",
  "license": "MIT",
  "dependencies": {
    "acorn": "^6.0.5",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.3.1",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@svgr/parcel-plugin-svgr": "^4.1.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^3.6.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-unicorn": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.11.0",
    "prettier": "^1.16.0",
    "react-snap": "^1.23.0",
    "rimraf": "^2.6.3"
  }
}
